{
  "extends": "next/core-web-vitals",
  "rules": {
    "semi": [
      "error",
      "always"
    ],
    "react/no-unescaped-entities": "off",
    "no-unused-vars": "warn",
    "no-console": "off",
    "eqeqeq": [
      "error",
      "always"
    ],
    "curly": "error",
    "complexity": [
      "warn",
      {
        "max": 15
      }
    ],
    "no-trailing-spaces": "error",
    "eol-last": [
      "error",
      "always"
    ]
  }
}